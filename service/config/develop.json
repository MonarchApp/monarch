{
  "auth": {
    "jwtAudience": "uiUrl",
    "saltRounds": 12
  },
  "connection": {
    "host": "localhost",
    "port": 8080,
    "routes": {
      "cors": true
    }
  },
  "good": {
    "ops": {
      "interval": 5000
    },
    "reporters": {
      "console": [{
        "module": "good-squeeze",
        "name": "Squeeze",
        "args": [{
          "log": "*",
          "response": "*"
        }]
      }, {
        "module": "good-console",
        "args": [{
          "format": "MM/DD/YYYY: HH:mm:ss.SSS"
        }]
      }, "stdout"]
    }
  }
}
